class Employee {
		String empName;
		int empNo;
		float salary;
		String empDesig;
		byte expYrs;
		
		void setEmployee(String Name,int no,float sal,String des,byte ex)
		{
			empName = Name;
			empNo = no;
			salary = sal;
			empDesig = des;
			expYrs = ex;
			
		}
		void getEmployee()
		{
			System.out.println(empName+"\t"+empNo+"\t"+empDesig+"\t"+salary+"\t"+expYrs+"\n");
			
		}
}
class demoEmployee
{
	public static void main(String args[])
	{
		Employee priya = new Employee();
		priya.setEmployee("priyanka",917123,88888.0f,"PM",(byte)20);
		System.out.println("empName\tempNo\tempDesig\tsalary\texpYrs\n");
		priya.getEmployee();
		Employee riya= new Employee();
		riya.empName = "riya";
		riya.empNo = 783246;
		riya.salary = 6754927.0f;
		riya.empDesig = "Project Lead";
		riya.expYrs = (byte)10;
